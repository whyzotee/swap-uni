{
  "files.exclude": {
    "**/.cursor": true,
    "**/.expo": true,
    "**/node_modules": true,
    "**/*.d.ts": true,
    "LICENSE": true,
    "**/*.env": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit"
  }
}
